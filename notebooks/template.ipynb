    "import os\n",
    "\n",
    "def get_kaggle_input_dir() -> str:\n",
    "    for path in [\n",
    "        '/kaggle/input',\n",
    "        '../kaggle/input',\n",
    "    ]:\n",
    "        if os.path.exists(path):\n",
    "            return path\n",
    "    raise FileNotFoundError(\"Kaggle input directory not found.\")\n",
    "\n",
    "get_kaggle_input_dir()"
   ]
